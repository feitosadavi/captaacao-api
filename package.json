{
  "name": "captaacao-api",
  "version": "0.2.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/main/server.js NODE_ENV=production",
    "compile": "rimraf build && tsc -p tsconfig-build.json && node build/main/server.js",
    "dev": "nodemon --watch 'src/' --exec \"ts-node-dev -r tsconfig-paths/register src/main/server.ts\" -e ts",
    "debug": "nodemon -L --watch ./build --inspect=0.0.0.0:9222 --nolazy ./build/main/server.js",
    "build": "rimraf build && tsc -p tsconfig-build.json",
    "docker:build": "sudo docker build . -t feitosadavi/captacao-api:latest",
    "docker:pull": "docker pull feitosadavi/captacao-api:latest",
    "up": "npm run build && sudo docker-compose up -d",
    "down": "sudo docker-compose down",
    "playground": "nodemon playground/index.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --watch --passWithNoTests --runInBand /home/eu/Documents/captaacao-api/tests/infra/db/account-mongo-repository.spec.ts",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:clear": "jest --clearCache"
  },
  "keywords": [],
  "author": "Davi Feitosa",
